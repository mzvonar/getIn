  },
  "homepage": "https://github.com/mzvonar/getIn#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.1",
    "@babel/preset-env": "7.0.0",
    "coveralls": "3.0.2",
    "expect": "1.20.2",
    "istanbul": "0.4.5",
    "mocha": "5.2.0"
